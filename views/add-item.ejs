        <div class="input-field col s6">
          <i class="material-icons prefix">shopping_cart</i>
          <input id="item_name" type="text" class="validate" name="name"  placeholder="Item Name">
          
        </div>
        <div class="input-field col s6">
         <i class="material-icons prefix">request_quote</i>
          <input id="icon_telephone" type="tel" class="validate" name="price" placeholder="price">
          
        </div>
      </div>
      <button type="submit" class="btn btn-primary">Add Product</button>
    </form>
  </div>
  </div>

  
  
</body>
</html>